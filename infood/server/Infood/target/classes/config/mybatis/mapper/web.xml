<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="web">

	<!-- 파일 업로드 -->
	<insert id="upload" parameterType="foodVO">
		insert into content_food values(
			0,
			#{user_idx},
			#{user_nikname},
			#{image},
			#{subway},
			#{food},
			#{content},
			current_timestamp
		)
	</insert>
	
</mapper>












